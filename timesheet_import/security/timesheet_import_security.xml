<odoo>
    <data>

        <!-- Define access rights for timesheet.import.wizard -->
        <record id="model_timesheet_import_wizard_access" model="ir.model.access">
            <field name="name">timesheet.import.wizard access</field>
            <field name="model_id" ref="model_timesheet_import_wizard"/>
            <field name="group_id" ref="base.group_user"/>
            <field name="perm_read" eval="1"/>
            <field name="perm_write" eval="1"/>
            <field name="perm_create" eval="1"/>
            <field name="perm_unlink" eval="1"/>
        </record>

        <record id="timesheet_import_rule" model="ir.rule">
            <field name="name">Allow timesheet import for users</field>
            <field name="model_id" ref="model_timesheet_import_wizard"/>
            <field name="groups" eval="[(4, ref('base.group_user'))]"/>
            <field name="domain_force">[('id', '!=', False)]</field>
        </record>

    </data>
</odoo>
